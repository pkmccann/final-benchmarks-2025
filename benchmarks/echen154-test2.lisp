(define (nested-ifs x y z)
    (if (< x y)
    (if (< y z)
    (+ x (+ y z))
    (if (= y z)
    (* y 2)
    (- y z)))
    (if (= x y)
    (if (= y z)
    (* x -1)
    (+ x z))
    (if (< z x)
    (- x z)
    (if (< y z)
    (+ y z)
    (* z y))))))

(let ((call1 (nested-ifs 1 2 3)))
  (let ((call2 (nested-ifs 1 1 2)))
    (let ((call3 (nested-ifs 2 1 1)))
      (print (+ call1 (+ call2 call3))))))
